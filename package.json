{
  "name": "qlbuilder",
  "version": "2.8.3",
  "description": "CLI helper tool for Qlik Sense developers to allow write script locally and update in remote app",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "preferGlobal": true,
  "bin": {
    "qlbuilder": "./bin/qlbuilder.js"
  },
  "files": [
    "!.env",
    "dist",
    "funding.yml",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build-debug": "rollup -c rollup.config.debug.js",
    "start": "node out/app.js",
    "prestart": "npm run build-tsc",
    "build:tsc": "tsc",
    "test": "mocha --require ts-node/esm test/**/*.spec.js --exclude test/_playground.spec.js"
  },
  "homepage": "https://github.com/informatiqal/qlbuilder",
  "repository": {
    "type": "git",
    "url": "https://github.com/informatiqal/qlbuilder.git"
  },
  "keywords": [
    "qlik",
    "sense",
    "build",
    "script",
    "reload"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "cli-spinner": "^0.2.10",
    "commander": "^14.0.0",
    "enigma-mixin": "^1.7.0",
    "enigma.js": "^2.14.0",
    "filenamify": "^6.0.0",
    "js-yaml": "^4.1.0",
    "natural-orderby": "^3.0.2",
    "prompts": "^2.4.2",
    "qlik-sense-authenticate": "^0.1.13",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=16.0"
  },
  "devDependencies": {
    "@gmrchk/cli-testing-library": "0.1.2",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/chai": "5.2.2",
    "@types/cli-spinner": "0.2.3",
    "@types/enigma.js": "2.2.5",
    "@types/js-yaml": "4.0.9",
    "@types/mocha": "10.0.10",
    "@types/node": "24.0.3",
    "@types/qlik-engineapi": "12.67.16",
    "@types/ws": "8.18.1",
    "chai": "5.2.0",
    "dotenv": "16.5.0",
    "mocha": "11.7.0",
    "node-fetch": "^3.3.2",
    "nyc": "17.1.0",
    "rollup": "^4.44.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  }
}
