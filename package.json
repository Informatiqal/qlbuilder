{
  "name": "qlbuilder",
  "version": "2.0.1",
  "description": "CLI helper tool for Qlik Sense developers to allow write script locally and update in remote app",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "type": "module",
  "main": "./dist/index.js",
  "preferGlobal": true,
  "bin": {
    "qlbuilder": "./bin/qlbuilder.js"
  },
  "scripts": {
    "build": "rollup -c",
    "build-debug": "rollup -c rollup.config.debug.js",
    "start": "node out/app.js",
    "prestart": "npm run build",
    "build:tsc": "tsc",
    "test": "mocha --require ts-node/esm test/**/*.spec.js"
  },
  "homepage": "https://github.com/informatiqal/qlbuilder",
  "repository": {
    "type": "git",
    "url": "https://github.com/informatiqal/qlbuilder.git"
  },
  "keywords": [
    "qlik",
    "sense",
    "build",
    "script",
    "reload"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "cli-spinner": "^0.2.10",
    "commander": "^9.4.0",
    "enigma.js": "^2.9.0",
    "filenamify": "^5.1.1",
    "js-yaml": "^4.1.0",
    "natural-orderby": "^2.0.3",
    "qlik-sense-authenticate": "^0.1.13",
    "rimraf": "^3.0.2",
    "ws": "^8.8.1"
  },
  "engines": {
    "node": ">=12.20"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "4.1.0",
    "jest": "29.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-delete": "2.0.0",
    "@types/axios": "0.14.0",
    "@types/chai": "4.3.3",
    "@types/chalk": "2.2.0",
    "@types/chokidar": "2.1.3",
    "@types/cli-spinner": "0.2.1",
    "@types/commander": "2.12.2",
    "@types/enigma.js": "2.2.2",
    "@types/js-yaml": "4.0.5",
    "@types/mocha": "10.0.0",
    "@types/node": "18.7.9",
    "@types/qlik-engineapi": "12.67.12",
    "@types/ws": "8.5.3",
    "chai": "4.3.6",
    "dotenv": "16.0.3",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.34.0",
    "ts-node": "10.9.1"
  }
